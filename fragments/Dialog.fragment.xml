<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="PlantsFragment"
		noDataText="No Plants Found"
		title="Select Production Plant"
		search="handlePlantSearch"
		confirm="handleClose"
		close="handleClose"
		items="{
			path: '/PlantSet'
		}" >
		
		<StandardListItem
			title="Plant {PlantID}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>