		navButtonPress="handleNavButtonPress"
    id="idFormContainer"
    showHeader="true" >
    <footer>
      <Bar>
        <contentRight>
          <Button id="idButtonSave" text="Save" type="Emphasized" visible="true" press="handleFooterBarButtonPress" />
          <Button id="idButtonCancel" text="Cancel" visible="true" press="handleFooterBarButtonPress" />
 		  <Button id="idButtonEdit" text="Edit" visible="false" press="handleFooterBarButtonPress" />
        </contentRight>
      </Bar>
    </footer>
	</Page>
